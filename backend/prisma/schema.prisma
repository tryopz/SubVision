// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../prisma/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Word {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
